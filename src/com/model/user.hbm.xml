<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "hibernate-hbm_cavers-3.0.dtd" >

<hibernate-mapping package="com.model">
	<class name="user" table="users">
		<id name="id" type="java.lang.Integer">
			<column name="id"></column>
			<generator class="native"></generator>
		</id>
		<property name="userName" type="java.lang.String" column="userName"></property>
		<property name="password" type="java.lang.String" column="password"></property>
		<property name="name" type="java.lang.String" column="name"></property>
		<property name="sex" type="java.lang.String" column="sex"></property>
		<property name="phone" type="java.lang.String" column="phone"></property>
		<property name="qq" type="java.lang.String" column="QQ"></property>
	</class>
</hibernate-mapping>